{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@app/utils": "<rootDir>/../libs/utils/src",
    "@app/omdb/(.*)": "<rootDir>/../libs/omdb/src/$1",
    "@app/omdb": "<rootDir>/../libs/omdb/src",
    "@app/mongodb-memory-server/(.*)": "<rootDir>/../libs/mongodb-memory-server/src/$1",
    "@app/mongodb-memory-server": "<rootDir>/../libs/mongodb-memory-server/src"
  }
}